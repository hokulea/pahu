{
  "name": "@hokulea/pahu",
  "version": "0.0.0",
  "description": "Hokulea Headless Forms",
  "license": "MIT",
  "author": "Thomas Gossmann",
  "repository": "https://github.com/hokulea/hokulea",
  "files": [
    "dist"
  ],
  "type": "module",
  "imports": {
    "#src": "./src/index.ts",
    "#src/*": "./src/*",
    "#ember": "./src/emmber.ts",
    "#tests/*": "./tests/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "tsdown --watch",
    "test": "vitest run",
    "test:ui": "vitest --browser.provider=preview --no-browser.headless --browser.name=firefox",
    "x:vanilla": "vite dev examples/vanilla"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "dot-prop": "^9.0.0"
  },
  "peerDependencies": {
    "ember-modifier": "4.2.2",
    "ember-resources": "^7.0.6"
  },
  "peerDependenciesMeta": {
    "ember-modifier": {
      "optional": true
    },
    "ember-resources": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.6",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/test-helpers": "5.2.2",
    "@embroider/macros": "^1.18.1",
    "@embroider/core": "^4.1.3",
    "@embroider/vite": "^1.1.6",
    "@glint/core": "^2.0.0-alpha.4",
    "@glint/environment-ember-loose": "^2.0.0-alpha.3",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.3",
    "@glint/template": "^1.6.0-alpha.2",
    "@gossi/config-eslint": "^1.3.0",
    "@gossi/config-prettier": "^1.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@testing-library/dom": "10.4.1",
    "@types/qunit": "^2.19.13",
    "@types/sinon": "^17.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "babel-plugin-ember-template-compilation": "^3.0.0",
    "ember-modifier": "4.2.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-resources": "^7.0.6",
    "ember-source": "~6.5.0",
    "ember-template-imports": "4.3.0",
    "eslint": "^9.30.1",
    "playwright": "^1.54.2",
    "prettier": "^3.5.3",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "sinon": "^21.0.0",
    "tsdown": "^0.14.1",
    "type-fest": "^4.41.0",
    "typescript": "5.9.2",
    "valibot": "^1.1.0",
    "vite": "^7.1.3",
    "vitest": "3.2.4",
    "vitest-browser-html": "^0.1.3"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  }
}
